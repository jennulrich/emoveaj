{% spaceless %}
<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <title>{% block tite 'Admin E-Move' %}</title>

    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="{{ asset('template-emove/font-awesome/css/font-awesome.min.css') }}">
    <!-- Style Admin -->
    <link rel="stylesheet" href="{{ asset('template-emove/css/style-admin.css') }}">
    <!-- Theme style -->
    <link rel="stylesheet" href="{{ asset('template-emove/css/adminlte.min.css') }}">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <!-- Datatable -->
    <link href="http://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">

</head>
<body class="hold-transition sidebar-mini" style="background-color: #f4f6f9;">
    <div class="wrapper">

        {% include 'admin/header.html.twig' %}

        {% include 'admin/sidebar.html.twig' %}

        <!-- Div de contenu -->
        <div class="content-wrapper">
            <div class="content">
                <div class="container-fluid">
                    {% block content '' %}
                </div>
            </div>
        </div>
        <!-- End Div de contenu -->

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
            <div class="p-3">
                <h5>Title</h5>
                <p>Sidebar content</p>
            </div>
        </aside>
        <!-- /.control-sidebar -->

        {% include 'admin/footer.html.twig' %}

    </div>

    <!-- jQuery -->
    <script src="{{ asset('template-emove/js/jquery.min.js') }}"></script>
    <!-- AdminLTE App -->
    <script src="{{ asset('template-emove/js/adminlte.js') }}"></script>
    <!-- Bootstrap 4 -->
    <script src="{{ asset('template-emove/js/bootstrap.bundle.min.js') }}"></script>
    <!-- Datatable -->
    <script src="http://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>

    {% block javascripts '' %}
</body>
</html>
{% endspaceless %}

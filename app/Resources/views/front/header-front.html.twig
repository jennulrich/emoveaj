<header class="site-header header-transparent">
    <div class="top-bar">
        <div class="container">
            <div class="row">
                <div class="dlab-topbar-left">
                    <ul>
                        <li><a href="{{ path('user_registration') }}" >Inscription</a></li>
                        <li> | </li>
                        <li><a href="{{ path('user_login') }}" >Connexion</a></li>
                    </ul>
                </div>
                <div class="dlab-topbar-right topbar-social">
                    <ul>
                        <li>
                            <a href="javascript:void(0);"><i class="fa fa-envelope-o"></i> support@emove.com</a>
                        </li>
                        <li><a href="#" class="site-button-link facebook hover"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#" class="site-button-link twitter hover"><i class="fa fa-twitter"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- main header -->
    <div class="sticky-header main-bar-wraper">
        <div class="main-bar clearfix ">
            <div class="container clearfix">
                <!-- website logo -->
                <div class="logo-header mostion">
                    <a href="#"><img src="{{ asset('template-emove/img/emovelogo.png') }}" class="logo" alt="logo" style="height: 65px;"></a>
                </div>
                <!-- nav toggle button -->
                <button data-target=".header-nav" data-toggle="collapse" type="button" class="navbar-toggle collapsed" aria-expanded="false" >
                    <i class="fa fa-bars"></i>
                </button>
                <!-- extra nav -->
                <div class="extra-nav">
                    <div class="extra-cell">
                        <button id="quik-search-btn" type="button" class="site-button-link"><i class="fa fa-search"></i></button>
                    </div>
                </div>
                <!-- Quik search -->
                <div class="dlab-quik-search bg-primary ">
                    <form action="#">
                        <input name="search" value="" type="text" class="form-control" placeholder="Type to search">
                        <span id="quik-search-remove"><i class="fa fa-close"></i></span>
                    </form>
                </div>
                <!-- main nav -->
                <div class="header-nav navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li class="has-mega-menu
                            {%  if app.request.attributes.get('_route') == 'home' %}active{% endif %}">
                            <a href="{{ path('home') }}">Accueil</a>
                        </li>
                        <li class="has-mega-menu
                            {%  if app.request.attributes.get('_route') == 'front_cars'
                                or app.request.attributes.get('_route') == 'front_view_car'
                            %}active{% endif %}">
                            <a href="{{ path('front_cars') }}">Voitures</a>
                        </li>
                        <li class="has-mega-menu
                            {%  if app.request.attributes.get('_route') == 'front_scooters'
                                or app.request.attributes.get('_route') == 'front_view_scooter'
                            %}active{% endif %}">
                            <a href="{{ path('front_scooters') }}">Scooters</a>
                        </li>
                        <li class="has-mega-menu {%  if app.request.attributes.get('_route') == 'aboutus' %}active{% endif %}">
                            <a href="{{ path('aboutus') }}">Ã€ Propos</a>
                        </li>
                        <li class="has-mega-menu {%  if app.request.attributes.get('_route') == 'contact' %}active{% endif %}">
                            <a href="{{ path('contact') }}">Contact</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</header>
